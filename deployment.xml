<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<package xmlns="http://www.zend.com/server/deployment-descriptor/1.0" version="1.0">
    <name>MassiApigilitySkel</name>
    <summary>Apigility Skeleton</summary>
    <description>Apigility Skeleton with support for Zend CD pipeline</description>
    <version>
        <release>1.0.0</release>
    </version>
    <appdir>data</appdir>
    <docroot>data/public</docroot>
